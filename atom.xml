---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Rosaxote's World Atlas</title>
 <link href="https://rosaxote.github.io/atom.xml" rel="self"/>
 <link href="https://rosaxote.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://rosaxote.github.io/</id>
 <author>
   <name>Rosaxote</name>
   <email>op.3no.2@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://rosaxote.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://rosaxote.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
